        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Min Profit</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="R" min="0" class="form-control" placeholder="Rs.">
            </div>        
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Min number of Staff Required</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="staff" min="30" class="form-control" placeholder="No. Of Staff(min 30 Persons)">
            </div>        
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Total Budget</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="budget" min="50000" class="form-control" placeholder="Rs.(min 50000Rs.)">
            </div>        
        </div>
