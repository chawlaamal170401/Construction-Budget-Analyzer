<div class="form">
  <h2 style="color: #1d2d50; text-align: center;">Fill Details</h2>
<form method="POST" action="/minform">
<div class="card crd">
    <div class="card-header crdhd">
      <h3>Planning</h3>
    </div>
    <div class="card-body crdbd">
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Enter Cost Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="cost11" min="1000" class="form-control" placeholder="Rs.(min 10Rs.)">
            </div>        
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Profit Earned Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="profit11" min="400" class="form-control" placeholder="Rs.(min 400Rs.)">
            </div>        
        </div>
    
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Staff Required Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="staff11" min="5" class="form-control" placeholder="No. of Persons(min 5 Persons)">
            </div>        
        </div>
    </div>
</div>
<div class="card crd">
    <div class="card-header crdhd">
      <h3>Business And Transportation</h3>
    </div>
    <div class="card-body crdbd">
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Enter Cost Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="cost22" min="1000" class="form-control" placeholder="Rs.(min 1000Rs.)">
            </div>        
        </div>
    
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Profit Earned Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="profit22" min="200" class="form-control" placeholder="Rs.(min 20Rs.)">
            </div>        
        </div>
    
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Staff Required Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="staff22" min="5" class="form-control" placeholder="No. of Persons(min 5 Person)">
            </div>        
        </div>
  </div>
</div>
<div class="card crd">
    <div class="card-header crdhd">
      <h3>Site Work And Finishing</h3>
    </div>
    <div class="card-body crdbd">
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Enter Cost Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="cost33" min="500" class="form-control" placeholder="Rs.(min 500Rs.)">
            </div>        
        </div>
    
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Profit Earned Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="profit33" min="100" class="form-control" placeholder="Rs.(min 100Rs.)">
            </div>        
        </div>
    
        <div class="form-row">
            <div class="form-group col-md-6">
              <label><h5>Staff Required Per Day</h5></label>
            </div>
            <div class="col-md-6">
                <input type="number" name="staff33" min="10" class="form-control" placeholder="No. of Persons(min 10 Persons)">
            </div>        
        </div>
  </div>
</div>

